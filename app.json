{
  "expo": {
    "name": "Ambience",
    "slug": "ambience",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xyz.ambient",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.600076203058-o4tj3nvkl9qh014nnc0fn0lvaffgkknj"
        }
      }
    },
    "android": {
      "package": "com.xyz.ambient",
      "googleServicesFile": "./google-services.json"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}